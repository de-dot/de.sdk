{
  "name": "de.sdk",
  "version": "0.0.1",
  "description": "Delidev SDK pack",
  "main": "./dist/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "author": "SCOREX Corporation",
  "private": false,
  "scripts": {
    "compile": "rimraf ./dist && tsc && cp ./src/types/** ./dist",
    "prepack": "yarn run compile",
    "test:access": "yarn compile && mocha test/access.test.js",
    "test:dclient": "yarn compile && mocha test/dclient.test.js",
    "test:order": "yarn compile && mocha test/order.test.js"
  },
  "dependencies": {
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/request": "^2.48.8",
    "@types/request-promise": "^4.1.48",
    "expect.js": "^0.3.1",
    "mocha": "^10.2.0",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.2"
  },
  "directories": {
    "dist": "dist/",
    "docs": "docs/",
    "test": "test/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/de-livery/de.sdk"
  },
  "keywords": []
}
