{
  "name": "globe-sdk",
  "version": "1.0.11",
  "description": "Globe Ecosystem SDK",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "author": "Fabrice Marlboro",
  "private": false,
  "scripts": {
    "compile": "rimraf ./dist && tsc",
    "prepack": "yarn run compile",
    "test": "yarn run compile && yarn run test:types && yarn run test:unit",
    "test:unit": "nyc mocha --require ts-node/register --reporter spec --slow 200 --bail --timeout 10000 test/iframe.io.ts",
    "test:types": "tsd",
    "test:all": "mocha test",
    "test:wps:connect": "mocha test/wps.connect.test",
    "test:authorizer": "mocha test/authorizer.test",
    "test:wps:api": "mocha test/wps.api.test",
    "test:bnd": "mocha test/bnd.test",
    "test:csa": "mocha test/csa.test",
    "test:cas": "mocha test/cas.test"
  },
  "dependencies": {
    "aws-sdk": "^2.1148.0",
    "crypto-js": "^4.1.1",
    "express": "^4.17.1",
    "mongodb": "^4.1.4",
    "parseurl": "^1.3.3",
    "rand-token": "^1.0.1",
    "request": "^2.88.2",
    "socket.io-client": "^4.3.2",
    "tinify": "^1.6.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/parseurl": "^1.3.1",
    "@types/request": "^2.48.8",
    "expect.js": "^0.3.1",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "directories": {
    "example": "example/",
    "test": "test/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/pws-hub/globe-sdk"
  },
  "keywords": [
    "realtime",
    "iframe",
    "browser",
    "events",
    "io"
  ]
}
